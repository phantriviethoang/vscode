{
  "name": "carbon-canary",
  "displayName": "carbon-canary",
  "description": "carbon-canary",
  "version": "1.0.8",
  "engines": {
    "vscode": "^1.105.0"
  },
  "publisher": "phantriviethoang",
  "repository": {
    "type": "git",
    "url": "https://github.com/phantriviethoang/theme"
  },
  "icon": "images/logo.jpg",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "configurationDefaults": {
      "breadcrumbs.enabled": false,
      "editor.renderLineHighlight": "none",
      "scm.diffDecorations": "none",
      "editor.glyphMargin": false,
      "editor.fontSize": 15,
      "editor.lineHeight": 30,
      "editor.fontFamily": "JetBrains Mono, MonoLisa",
      "editor.fontLigatures": "'ss03', 'calt', 'dlig'",
      "editor.detectIndentation": false,
      "editor.wordWrap": "wordWrapColumn",
      "editor.copyWithSyntaxHighlighting": false,
      "editor.linkedEditing": true,
      "editor.formatOnPaste": true,
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.autoClosingBrackets": "always",
      "editor.bracketPairColorization.enabled": false,
      "editor.autoClosingQuotes": "always",
      "editor.autoIndent": "advanced",
      "editor.autoIndentOnPaste": true,
      "editor.colorDecorators": false,
      "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
      "editor.lineNumbers": "off",
      "editor.minimap.enabled": false,
      "editor.minimap.maxColumn": 250,
      "editor.scrollbar.horizontal": "hidden",
      "editor.scrollbar.vertical": "hidden",
      "editor.codeLens": false,
      "editor.guides.indentation": false,
      "editor.inlayHints.enabled": "off",
      "editor.renderControlCharacters": false,
      "editor.overviewRulerBorder": false,
      "editor.hideCursorInOverviewRuler": true,
      "editor.selectionHighlight": false,
      "editor.semanticHighlighting.enabled": true,
      "editor.occurrencesHighlight": "off",
      "editor.matchBrackets": "never",
      "editor.lightbulb.enabled": "off",
      "editor.padding.top": 10,
      "editor.stickyScroll.enabled": false,
      "editor.cursorSurroundingLines": 5,
      "editor.cursorBlinking": "solid",
      "editor.cursorSmoothCaretAnimation": "on",
      "editor.smoothScrolling": true,
      "editor.tabCompletion": "on",
      "editor.suggestFontSize": 16,
      "editor.suggestLineHeight": 30,
      "editor.quickSuggestions": {
        "strings": true,
        "other": true
      },
      "editor.screenReaderAnnounceInlineSuggestion": false,
      "editor.accessibilitySupport": "off",
      "editor.multiCursorModifier": "ctrlCmd",
      "editor.gotoLocation.multipleReferences": "goto",
      "editor.gotoLocation.multipleDefinitions": "goto",
      "editor.gotoLocation.multipleDeclarations": "goto",
      "editor.gotoLocation.multipleImplementations": "goto",
      "editor.gotoLocation.multipleTypeDefinitions": "goto",
      "workbench.startupEditor": "none",
      "workbench.tips.enabled": false,
      "workbench.activityBar.location": "hidden",
      "workbench.sideBar.location": "right",
      "workbench.tree.indent": 12,
      "workbench.tree.enableStickyScroll": false,
      "workbench.tree.renderIndentGuides": "none",
      "workbench.list.smoothScrolling": true,
      "workbench.secondarySideBar.defaultVisibility": "hidden",
      "workbench.secondarySideBar.showLabels": false,
      "workbench.quickOpen.closeOnFocusLost": false,
      "workbench.layoutControl.enabled": false,
      "workbench.panel.showLabels": false,
      "workbench.editor.editorActionsLocation": "hidden",
      "workbench.navigationControl.enabled": false,
      "window.commandCenter": false,
      "workbench.colorTheme": "carbon",
      "workbench.iconTheme": "carbon",
      "workbench.productIconTheme": "carbon",
      "workbench.editor.scrollToSwitchTabs": true,
      "workbench.editor.decorations.badges": false,
      "workbench.editor.decorations.colors": false,
      "workbench.editor.enablePreviewFromCodeNavigation": true,
      "workbench.editor.highlightModifiedTabs": true,
      "workbench.editor.pinnedTabSizing": "compact",
      "workbench.editor.showTabs": "none",
      "workbench.statusBar.visible": false,
      "window.newWindowDimensions": "inherit",
      "window.title": "${activeEditorShort}",
      "window.density.editorTabHeight": "compact",
      "window.menuBarVisibility": "toggle",
      "window.customTitleBarVisibility": "never",
      "window.titleBarStyle": "native",
      "window.title": "${activeEditorShort}${separator}${activeFolderShort}",
      "files.autoSave": "afterDelay",
      "files.autoSaveDelay": 500,
      "files.trimTrailingWhitespace": true,
      "files.insertFinalNewline": true,
      "files.trimFinalNewlines": true,
      "files.associations": {
        ".php_cs": "php",
        ".php_cs.dist": "php"
      },
      "files.exclude": {
        "**/node_modules": true,
        "**/.gitignore": true,
        "**/.gitattributes": true,
        "**/.vscode": true,
        "**/.DS_Store": true,
        "**/vite.config.js": true,
        "**/package.json": true,
        "**/package-lock.json": true,
        "**/yarn.lock": true,
        "**/pnpm-lock.yaml": true,
        "**/bun.lockb": true,
        "**/eslint.config.*": true,
        "**/postcss.config.*": true,
        "**/README.md": true,
        "**/dist": true,
        "**/build": true
      },
      "fileutils.inputBox.pathType": "workspace",
      "fileutils.moveFile.typeahead.enabled": true,
      "explorer.compactFolders": false,
      "explorer.confirmDragAndDrop": false,
      "explorer.confirmPasteNative": false,
      "explorer.confirmDelete": false,
      "explorer.fileNesting.enabled": true,
      "explorer.fileNesting.patterns": {
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
      },
      "search.searchOnType": false,
      "search.smartCase": true,
      "search.useIgnoreFiles": false,
      "search.exclude": {
        "**/vendor/{[^l],?[^ai]}*": true,
        "**/public/{[^i],?[^n]}*": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/_ide_helper.php": true,
        "**/composer.lock": true,
        "**/package-lock.json": true,
        "storage": true,
        ".phpunit.result.cache": true
      },
      "terminal.integrated.fontSize": 15,
      "terminal.integrated.lineHeight": 1.1,
      "terminal.integrated.fontFamily": "JetBrains Mono, MonoLisa",
      "terminal.integrated.enableMultiLinePasteWarning": "never",
      "terminal.integrated.suggest.enabled": true,
      "terminal.integrated.suggest.showStatusBar": false,
      "terminal.integrated.tabs.location": "left",
      "terminal.integrated.cursorStyle": "block",
      "terminal.integrated.smoothScrolling": true,
      "terminal.integrated.stickyScroll.enabled": false,
      "terminal.integrated.fontLigatures.enabled": true,
      "terminal.integrated.shellIntegration.decorationsEnabled": "never",
      "terminal.integrated.tabs.title": "${cwdFolder}",
      "diffEditor.hideUnchangedRegions.enabled": true,
      "diffEditor.useInlineViewWhenSpaceIsLimited": false,
      "diffEditor.ignoreTrimWhitespace": false,
      "diffEditor.wordWrap": "on",
      "git.confirmSync": false,
      "problems.decorations.enabled": false,
      "chat.commandCenter.enabled": false,
      "chat.editor.wordWrap": "on",
      "security.workspace.trust.untrustedFiles": "open",
      "extensions.ignoreRecommendations": true,
      "emmet.triggerExpansionOnTab": true,
      "emmet.includeLanguages": {
        "blade": "html",
        "vue-html": "html",
        "vue": "html",
        "react": "html",
        "javascript": "html"
      },
      "php.suggest.basic": false,
      "php.format.rules.addCommaAfterLastArrayElement": false,
      "php.format.rules.blankLinesAfterOpenTag": 1,
      "php.format.rules.endWithNewLine": true,
      "php.format.rules.openBraceOnNewLineForAnonymousClasses": true,
      "php.format.rules.spaceBeforeColonInControlStatements": true,
      "php.format.rules.spaceBeforeColonInReturnType": false,
      "php.format.rules.spaceAfterUnaryNot": true,
      "javascript.suggest.paths": false,
      "typescript.suggest.paths": false,
      "html.format.extraLiners": "nul",
      "vetur.format.options.tabSize": 4,
      "vetur.format.options.useTabs": true,
      "vetur.format.defaultFormatter.ts": "vscode-typescript",
      "vetur.format.defaultFormatter.html": "js-beautify-html",
      "prettier.tabWidth": 4,
      "prettier.useTabs": true,
      "prettier.singleAttributePerLine": true,
      "tailwindCSS.includeLanguages": {
        "plaintext": "html"
      },
      "tailwindCSS.experimental.configFile": null,
      "auto-rename-tag.activationOnLanguage": [
        "*"
      ],
      "blade.format.enable": true,
      "blade.format.wrapAttributes": "auto",
      "liveServer.settings.CustomBrowser": "firefox",
      "quokka.automaticRestart": true,
      "quokka.fontSize": 16,
      "quokka.showOutputOnStart": false,
      "quokka.useOutputPanel": false,
      "vim.cursorStylePerMode.visualblock": "block-outline",
      "vim.cursorStylePerMode.visualline": "block-outline",
      "vim.cursorStylePerMode.visual": "block-outline",
      "vim.camelCaseMotion.enable": true,
      "vim.easymotion": true,
      "vim.easymotionDimColor": "#888888",
      "vim.easymotionIncSearchForegroundColor": "#66cc66",
      "vim.easymotionKeys": "sadfjklewcmpgh",
      "vim.easymotionMarkerForegroundColorOneChar": "#ff6666",
      "vim.easymotionMarkerForegroundColorTwoCharFirst": "#ffaa33",
      "vim.easymotionMarkerForegroundColorTwoCharSecond": "#ffdd77",
      "vim.foldfix": true,
      "vim.highlightedyank.color": "#a9dc7660",
      "vim.highlightedyank.enable": true,
      "vim.hlsearch": true,
      "vim.joinspaces": false,
      "vim.leader": "<space>",
      "vim.registers.useClipboard": true,
      "vim.replaceWithRegister": true,
      "vim.searchMatchTextColor": "#bbbbbb",
      "vim.sneak": true,
      "vim.sneakUseIgnorecaseAndSmartcase": true,
      "vim.timeout": 700,
      "vim.useSystemClipboard": true,
      "vim.visualstar": true,
      "vim.vimrc.path": "~/.vimrc",
      "vim.vimrc.enable": true,
      "vim.handleKeys": {
        "<C-b>": true,
        "<C-c>": true,
        "<C-d>": true,
        "<C-h>": true,
        "<C-j>": true,
        "<C-m>": true,
        "<C-t>": true,
        "<C-v>": true
      },
      "vim.normalModeKeyBindings": [
        {
          "before": [
            "<leader>",
            "/"
          ],
          "commands": [
            "periscope.searchCurrentFile"
          ]
        },
        {
          "before": [
            "<leader>",
            "h"
          ],
          "commands": [
            "editor.action.showHover"
          ],
          "silent": true
        },
        {
          "before": [
            "<leader>",
            "n"
          ],
          "commands": [
            "explorer.newFile"
          ],
          "silent": true
        },
        {
          "before": [
            "<leader>",
            "q"
          ],
          "commands": [
            "workbench.action.closeActiveEditor"
          ],
          "silent": true
        },
        {
          "before": [
            "<leader>",
            "f"
          ],
          "commands": [
            "editor.action.formatSelection.multiple"
          ]
        }
      ],
      "vim.visualModeKeyBindingsNonRecursive": [
        {
          "before": [
            "<leader>",
            "f"
          ],
          "commands": [
            "editor.action.formatSelection.multiple"
          ]
        }
      ],
      "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
      },
      "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
      },
      "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
      },
      "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
      },
      "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
      },
      "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
      },
      "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
      },
      "[php]": {
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
      },
      "[snippets]": {
        "editor.defaultFormatter": "vscode.json-language-features"
      },
      "vscode_custom_css.imports": [
        "file://${userHome}/.config/Code/custom.css"
      ],
      "vue.autoInsert.dotValue": true,
      "vue.editor.templateInterpolationDecorators": false,
      "vue.format.wrapAttributes": "force-expand-multiline",
      "Laravel.bladeComponent.hover": false,
      "bladeFormatter.format.noMultipleEmptyLines": true,
      "bladeFormatter.misc.dontShowNewVersionMessage": true,
      "bladeFormatter.format.sortHtmlAttributes": "custom",
      "bladeFormatter.format.sortTailwindcssClasses": true,
      "bladeFormatter.format.wrapAttributes": "force-expand-multiline",
      "cSpell.userWords": [
        "bootcamp",
        "chakra",
        "checkin",
        "checkins",
        "clsx",
        "Codegen",
        "datadog",
        "Datetime",
        "dayjs",
        "Dotenv",
        "Elysia",
        "esbuild",
        "fastify",
        "Fastify",
        "feedbackwidget",
        "ffprobe",
        "Hasher",
        "Hono",
        "ilike",
        "IUGU",
        "jamjuree",
        "jupiter",
        "liveblocks",
        "LIVEBLOCKS",
        "Marguerita",
        "middlewares",
        "mixpanel",
        "monaco",
        "nestjs",
        "omni",
        "Omni",
        "Onboarded",
        "pallas",
        "postgres",
        "postgresql",
        "prefetch",
        "reactflow",
        "roboto",
        "rocketseat",
        "rotion",
        "rsxp",
        "Sandpack",
        "shiki",
        "skylab",
        "sqlite",
        "supergraph",
        "svgr",
        "sympla",
        "tailwindcss",
        "textblock",
        "tiptap",
        "trpc",
        "TRPC",
        "tsup",
        "unfollow",
        "Unfollow",
        "unform",
        "Unform",
        "unmark",
        "upsert",
        "Usuario",
        "WEBPUSH",
        "alpinejs",
        "enderror",
        "endforeach",
        "endforelse",
        "endif",
        "endisset",
        "endsection",
        "forelse",
        "isset",
        "Laracasts",
        "laravel",
        "linecap",
        "linejoin",
        "vite",
        "extralight",
        "Bobo",
        "tailwind"
      ],
      "splitHTMLAttributes.closingBracketOnNewLine": true,
      "splitHTMLAttributes.sortOrder": [
        [
          "^v-if",
          "^v-else",
          "^v-show",
          "^v-model",
          "^v-for",
          "^:key",
          "^key",
          "^v-",
          "^:",
          "^@click",
          "^@",
          "^id",
          "^class",
          "^.*=\""
        ]
      ],
      "splitHTMLAttributes.tabSize": 4,
      "chat.mcp.gallery.enabled": true
    },
    "themes": [
      {
        "label": "carbon",
        "uiTheme": "vs-dark",
        "path": "./themes/carbon.json"
      }
    ],
    "iconThemes": [
      {
        "id": "carbon",
        "label": "carbon",
        "path": "./icons/icon.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "carbon",
        "label": "carbon",
        "path": "./product-icon-themes/product-icon-theme.json"
      }
    ]
  },
  "keywords": [
    "theme",
    "icon",
    "dark"
  ],
  "license": "MIT"
}
